class Solution {
  public:
    bool arraySortedOrNot(vector<int>& arr) {
        // code here
        int i;
        for(i=0;i<arr.size()-1;i++)
        {
            if(arr[i] > arr[i+1])
               return false;
        }
        
        return true;
    }
};
//Time Complexity: O(n)Auxiliary Space: O(1)
